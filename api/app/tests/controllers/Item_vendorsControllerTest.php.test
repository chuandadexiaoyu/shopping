<?php

class ItemVendorsControllerTest extends TestCase {
	public function testIndex()
	{
		$response = $this->call('GET', 'item_vendors');
		$this->assertTrue($response->isOk());
	}

	public function testShow()
	{
		$response = $this->call('GET', 'item_vendors/1');
		$this->assertTrue($response->isOk());
	}

}
